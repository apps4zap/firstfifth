//Character track schema

export default {
  title: 'Track',
  name: 'pTrack',
  type: 'file',
  fields: [
    {
      name: 'description',
      type: 'string',
      title: 'Description'
    },
    {
      name: 'artist',
      type: 'reference',
      title: 'Artist',
      to: {type: 'person'}
    }
  ]
}
